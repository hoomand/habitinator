<div class="card">
  <div class="card-header d-flex align-items-center">
    <h3 class="h4">Users</h3>
  </div>
  <div class="card-body">
    <div class="table-responsive table-striped">
      <table class="table">
        <thead>
        <tr>
          <th>Username</th>
          <th>Sign In Count</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Role</th>
          <th>Creation Date</th>
          <th>Last Sign In</th>
        </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.sign_in_count %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.role %></td>
          <td><%= user.creation_date %></td>
          <td><%= user.last_sign_in_datetime %></td>
        </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

